        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.bonga.IHearYou"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "df47c44d-ef86-4de8-96b2-c42dca01feec"
      }
    },
    "owner": "lumuli"
  }
